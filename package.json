{
  "name": "order-me-pizza",
  "version": "0.1.0",
  "description": "A stupid service providing the ability for people to easily buy you a pizza delivery",
  "main": "dist/index.js",
  "scripts": {
    "build": "bower install --allow-root && babel lib -d dist",
    "clean": "rm -rf dist",
    "dev": "nodemon lib/index.js --exec babel-node",
    "postinstall": "npm run build && npm prune",
    "start": "forever start dist/index.js",
    "stop": "forever stop dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rkoval/order-me-pizza.git"
  },
  "engines": {
    "npm": "3.7.1",
    "node": "5.6.0"
  },
  "keywords": [
    "pizza",
    "dominos",
    "order",
    "delivery"
  ],
  "author": "Ryan Koval <ryan@ryankoval.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rkoval/order-me-pizza/issues"
  },
  "homepage": "https://github.com/rkoval/order-me-pizza#readme",
  "devDependencies": {
    "babel-register": "^6.5.2",
    "express-jade": "0.0.3",
    "nodemon": "^1.8.1"
  },
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "compression": "^1.6.1",
    "config": "^1.19.0",
    "dominos": "^2.0.1",
    "eslint": "^2.1.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "forever": "^0.15.1",
    "jade": "^1.11.0",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "morgan": "^1.6.1",
    "paypal-rest-sdk": "^1.6.8",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.2",
    "uuid": "^2.0.1"
  }
}
